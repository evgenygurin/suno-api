{
  "name": "r2r-mcp-server",
  "version": "1.0.0",
  "description": "R2R RAG Agent with MCP integration for Suno API project",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "cli": "tsx src/cli/index.ts",
    "ingest": "tsx src/ingestion/pipeline.ts",
    "test": "vitest",
    "test:integration": "vitest run tests/integration",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "r2r",
    "rag",
    "mcp",
    "ai-agent",
    "documentation",
    "knowledge-graph"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "dotenv": "^16.4.5",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "zod": "^3.23.8",
    "commander": "^12.1.0",
    "yaml": "^2.6.1",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "@typescript-eslint/parser": "^8.18.2",
    "@typescript-eslint/typescript-estree": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
